{
  "manifest_version": 2,
  "name": "URL Scraper",
  "version": "1.0",
  "description": "Scrapes the URLs of all links on a webpage.",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "URL Scraper"
  }
}
